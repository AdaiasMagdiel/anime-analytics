;; shadow-cljs configuration
{:source-paths
 ["src"]

 :dependencies
 [[reagent "1.2.0"]
  [cljs-http "0.1.46"]]

 :dev-http {8080 "public"}

 :builds
 {:app {:target :browser
        :output-dir "public/js"
        :asset-path "/js"
        :modules {:main {:init-fn com.adaiasmagdiel.app/init!}}
        :devtools {:after-load com.adaiasmagdiel.app/reload!}}}}
